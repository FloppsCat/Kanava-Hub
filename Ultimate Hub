local function HRWMN_fake_script() -- PlrTPButton.LocalScript 
	local script = Instance.new('LocalScript', PlrTPButton)

	script.Parent.MouseButton1Click:Connect(function()
		local partialName = script.Parent.Parent.TextBox.Text:lower() -- Получаем частичное имя
		local players = game:GetService("Players"):GetPlayers()
		local targetPlayer
	
		for _, player in ipairs(players) do
			if player ~= game.Players.LocalPlayer and player.Name:lower():sub(1, #partialName) == partialName then
				targetPlayer = player
				break
			end
		end
	
		if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
			local myChar = game.Players.LocalPlayer.Character
			if myChar and myChar:FindFirstChild("HumanoidRootPart") then
				myChar.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame
			end
		end
	end)
end
coroutine.wrap(HRWMN_fake_script)()
local function IAXBDC_fake_script() -- PVPButton.LocalScript 
	local script = Instance.new('LocalScript', PVPButton)

	local page1 = script.Parent.Parent.TPPage
	local page2 = script.Parent.Parent.PVPPage
	script.Parent.MouseButton1Click:Connect(function()
		page1.Visible = false
		page2.Visible = true
	end)
end
coroutine.wrap(IAXBDC_fake_script)()
local function SXLFV_fake_script() -- AttackButton.LocalScript 
	local script = Instance.new('LocalScript', AttackButton)

	local pvpbutton = script.Parent
	local txt = script.Parent.Parent.NameBox
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local player = Players.LocalPlayer
	
	local FOLLOW_DURATION = 1 
	local HIT_INTERVAL = 0.2
	local FOLLOW_DISTANCE = 0.5
	local HEIGHT_OFFSET = 0.2
	
	local hitRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("HitRequest")
	
	local function executeAttack(targetPlayer)
		local character = player.Character
		if not character then return end
	
		local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
		if not humanoidRootPart then return end
	
		local originalCFrame = humanoidRootPart.CFrame
		local targetCharacter = targetPlayer.Character
		if not targetCharacter then return end
	
		local targetRoot = targetCharacter:FindFirstChild("HumanoidRootPart")
		if not targetRoot then return end
	
		local attackStart = os.clock()
		local lastHitTime = 0
	
		local connection
		connection = RunService.Heartbeat:Connect(function()
			local now = os.clock()
			if now - attackStart >= FOLLOW_DURATION then
				connection:Disconnect()
				humanoidRootPart.CFrame = originalCFrame
				return
			end
	
			-- Плавное перемещение
			local offset = targetRoot.CFrame.LookVector * -FOLLOW_DISTANCE
			local newPosition = targetRoot.Position + offset + Vector3.new(0, HEIGHT_OFFSET, 0)
			humanoidRootPart.CFrame = humanoidRootPart.CFrame:Lerp(
				CFrame.new(newPosition, targetRoot.Position),
				0.3
			)
	
			-- Атака с интервалом
			if now - lastHitTime >= HIT_INTERVAL then
				pcall(function() hitRemote:InvokeServer() end)
				lastHitTime = now
			end
		end)
	end
	
	-- Обработчик кнопки (без изменений)
	pvpbutton.MouseButton1Click:Connect(function()
		local targetName = txt.Text
		if targetName == "" then return end
	
		for _, plr in ipairs(Players:GetPlayers()) do
			if plr ~= player and plr.Name:lower():find(targetName:lower()) then
				executeAttack(plr)
				return
			end
		end
	end)
end
coroutine.wrap(SXLFV_fake_script)()
