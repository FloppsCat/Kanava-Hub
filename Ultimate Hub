-- Scripts:

local function GPBIVL_fake_script() -- TeleportsButton.LocalScript 
	local script = Instance.new('LocalScript', TeleportsButton)

	local page1 = script.Parent.Parent.TPPage
	local page2 = script.Parent.Parent.PVPPage
	script.Parent.MouseButton1Click:Connect(function()
		page1.Visible = true
		page2.Visible = false
	end)
end
coroutine.wrap(GPBIVL_fake_script)()
local function MROXFZ_fake_script() -- HubFrame.LocalScript 
	local script = Instance.new('LocalScript', HubFrame)

	script.Parent.Active = true
	script.Parent.Draggable = true
	
end
coroutine.wrap(MROXFZ_fake_script)()
